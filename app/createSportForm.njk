{% extends "base.njk" %}

{% block content %}
  <h1>Create a New Sport</h1>
<form action="/sports/create" method="post" enctype="application/x-www-form-urlencoded">
  <label for="nom">Sport Name:</label>
  <input type="text" id="nom" name="nom" required>
  <button type="submit">Create Sport</button>
</form>


{% endblock %}

{% block styles %}
  <link rel="stylesheet" type="text/css" href="/css/createEpreuve.css">
{% endblock %}


